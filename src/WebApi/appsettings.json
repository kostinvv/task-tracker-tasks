{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "Authority": "http://localhost:9090/realms/task-tracker",
        "Audience": [ "task-tracker-tasks", "account" ]
      }
    },
    "Keyclock": {
      "ClientId": "task-tracker-tasks",
      "ClientSecret": "<CLIENT_SECRET>",
      "Scope": [ "openid", "profile", "email" ]
    }
  },
  "AllowedHosts": "*"
}
